<section>
  <summary class="title">
      <%- post.title -%>
  </summary>
  <div class="date">
    <%- date(post.date, 'ddd YYYY-MM-DD HH:mm') %>
  </div>
  <p class="sub">
    <%- post.content -%>
  </p>
  <div class="tags">
    <% if (post.tags && post.tags.length) { %>
      <%- partial('post/tags', {tags: post.tags}) %>
        <% } %>
  </div>
  <%- partial('post/license',{at_path: page.permalink}) %>
  <%- partial('post/comment') %>
</section>